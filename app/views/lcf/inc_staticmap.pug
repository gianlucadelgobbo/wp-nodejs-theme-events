- var str = "";
each val, country in dateplace['wpcf-location']
   each val, city in dateplace['wpcf-location'][country]
       each val, venue in dateplace['wpcf-location'][country][city]
           - str+="&markers=size:mid%7Ccolor:0xff0000%7Clabel:1%7C"+ val.lng+","+val.lat
img.img-responsive(src="http://maps.googleapis.com/maps/api/staticmap?zoom=6&scale=false&size=100x75&maptype=roadmap&format=png&visual_refresh=true"+str)